<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.duongame.basicplayer.pro">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.duongame.basicplayer.lib.activity.MainActivity"
            android:configChanges="orientation|keyboardHidden|screenSize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.duongame.basicplayer.lib.activity.PlayerActivity"
            android:theme="@style/PlayerTheme"
            android:configChanges="orientation|keyboardHidden|screenSize">

        </activity>
        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="479ae7d868a353eb8da82e059fd1a04418af3cf3" />
    </application>

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="com.android.launcher.permission.INSTALL_SHORTCUT" />

</manifest>
